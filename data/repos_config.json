{
  "LINUX": {
    "url": "https://github.com/torvalds/linux",
    "branch": "master",
    "core_paths": ["kernel/", "mm/", "fs/", "sched/"],
    "ignore_paths": ["drivers/", "arch/", "tools/", "Documentation/", "samples/"]
  },
  "KUBERNETES": {
    "url": "https://github.com/kubernetes/kubernetes",
    "branch": "master",
    "core_paths": ["pkg/", "cmd/", "staging/"],
    "ignore_paths": ["test/", "docs/", "vendor/"]
  },
  "FREEBSD": {
    "url": "https://github.com/freebsd/freebsd-src",
    "branch": "main",
    "core_paths": ["sys/kern/", "sys/vm/", "sys/sys/"],
    "ignore_paths": ["sys/dev/", "contrib/", "tests/"]
  },
  "POSTGRES": {
    "url": "https://github.com/postgres/postgres",
    "branch": "master",
    "core_paths": ["src/backend/"],
    "ignore_paths": ["src/test/", "doc/"]
  },
  "REDIS": {
    "url": "https://github.com/redis/redis",
    "branch": "unstable",
    "core_paths": ["src/"],
    "ignore_paths": ["tests/", "deps/"]
  },
  "SQLITE": {
    "url": "https://github.com/sqlite/sqlite",
    "branch": "master",
    "core_paths": ["src/"],
    "ignore_paths": ["test/", "tool/"]
  },
  "NGINX": {
    "url": "https://github.com/nginx/nginx",
    "branch": "master",
    "core_paths": ["src/core/", "src/http/", "src/event/"],
    "ignore_paths": ["docs/", "misc/"]
  },
  "HTTPD_APACHE": {
    "url": "https://github.com/apache/httpd",
    "branch": "trunk",
    "core_paths": ["server/", "modules/http/"],
    "ignore_paths": ["docs/", "test/"]
  },
  "GCC": {
    "url": "https://github.com/gcc-mirror/gcc",
    "branch": "master",
    "core_paths": ["gcc/"],
    "ignore_paths": ["libgo/", "libjava/", "gcc/testsuite/"]
  },
  "LLVM": {
    "url": "https://github.com/llvm/llvm-project",
    "branch": "main",
    "core_paths": ["llvm/lib/", "clang/lib/"],
    "ignore_paths": ["llvm/test/", "clang/test/", "lldb/"]
